{
    "name": "viewstore-simple-demo",
    "version": "0.1.0",
    "private": false,
    "dependencies": {
        "autoprefixer": "7.1.6",
        "babel-core": "6.26.0",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react-app": "^3.1.0",
        "babel-runtime": "6.26.0",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "css-loader": "0.28.7",
        "director": "^1.2.8",
        "dotenv": "4.0.0",
        "eslint": "4.10.0",
        "eslint-config-react-app": "^2.0.1",
        "eslint-loader": "1.9.0",
        "eslint-plugin-flowtype": "2.39.1",
        "eslint-plugin-import": "2.8.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "7.4.0",
        "express": "^4.16.2",
        "express-graphql": "^0.6.11",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "1.1.5",
        "fs-extra": "3.0.1",
        "get-graphql-schema": "^2.1.1",
        "graphql": "^0.11.7",
        "graphql-relay": "^0.5.3",
        "history": "^4.7.2",
        "html-webpack-plugin": "2.29.0",
        "jest": "20.0.4",
        "material-ui": "^0.19.4",
        "mobx": "^3.3.1",
        "mobx-react": "^4.3.4",
        "mobx-utils": "^3.2.2",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "promise": "8.0.1",
        "prop-types": "^15.6.0",
        "raf": "3.4.0",
        "react": "^16.1.1",
        "react-dev-utils": "^4.2.1",
        "react-dom": "^16.1.1",
        "react-relay": "^1.4.1",
        "regenerator": "^0.11.0",
        "style-loader": "0.19.0",
        "superagent": "^3.8.1",
        "sw-precache-webpack-plugin": "0.11.4",
        "url-loader": "0.6.2",
        "webpack": "3.8.1",
        "webpack-manifest-plugin": "1.3.2",
        "whatwg-fetch": "2.0.3",
        "winston": "^3.0.0-rc1"
    },
    "scripts": {
        "build":
            "node ./scripts/build.js && NODE_ENV=production babel -d ./server/prod ./server/dev",
        "server:build":
            "NODE_ENV=production babel -d ./server/prod ./server/dev",
        "app:build": "node ./scripts/build.js",
        "start:dev": "NODE_ENV=development yarn babel-node ./server/dev",
        "start:prod": "NODE_ENV=production node ./server/prod",
        "api": "json-server server/mock.json -p 5000"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-relay": "^1.4.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "json-server": "^0.12.1",
        "nodemon": "^1.12.1",
        "relay-compiler": "^1.4.1",
        "rimraf": "^2.6.2",
        "webpack-dev-middleware": "^1.12.0",
        "webpack-hot-middleware": "^2.20.0"
    },
    "jest": {
        "collectCoverageFrom": ["src/**/*.{js,jsx,mjs}"],
        "setupFiles": ["<rootDir>/config/polyfills.js"],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)":
                "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "mjs",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "babel": {
        "presets": [
            "react-app",
            [
                "env",
                {
                    "targets": {
                        "browsers": ["last 2 versions", "IE >= 9"]
                    }
                }
            ]
        ],
        "plugins": [
            "relay",
            "transform-decorators-legacy",
            "transform-class-properties"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
